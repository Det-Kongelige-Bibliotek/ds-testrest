{
  "swagger": "2.0",
  "info": {
    "title": "Simple maping fail example",
    "version": "0.1.0"
  },
  "paths": {
    "/endpoint": {
      "post": {
        "summary": "accepts the autogenerated code, but doesnt map the object to the python dict",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "test_object",
            "description": "object that fails to map",
            "schema": {
              "$ref": "#/definitions/TestObject"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "allmost everything is OK"
          },
          "400": {
            "description": "invalid input, object invalid"
          },
          "409": {
            "description": "an existing item already exists"
          }
        }
      }
    }
  },
  "definitions": {
    "TestObject": {
      "type": "object",
      "required": [
        "property_one",
        "property_two",
        "property_three"
      ],
      "properties": {
        "property_one": {
          "type": "string"
        },
        "property_two": {
          "type": "string"
        },
        "property_three": {
          "type": "string"
        }
      }
    }
  },
  "host": "127.0.0.1",
  "basePath": "/",
  "schemes": [
    "http"
  ]
}